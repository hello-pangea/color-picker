{
  "name": "@hello-pangea/color-picker",
  "version": "3.2.2",
  "private": false,
  "description": "React color pickers from Sketch, Photoshop, Chrome, Github, Twitter & more",
  "main": "./dist/color-picker.cjs.js",
  "module": "./dist/color-picker.esm.js",
  "types": "./dist/color-picker.d.ts",
  "files": [
    "/dist",
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-pangea/color-picker.git"
  },
  "homepage": "https://colorpicker.hellopangea.com/",
  "bugs": {
    "url": "https://github.com/hello-pangea/color-picker/issues"
  },
  "author": "case <case@casesandberg.com>",
  "maintainers": [
    "Reece Carolan <reece@hellopangea.com>"
  ],
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "color picker",
    "react-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ],
  "scripts": {
    "test": "jest --config ./jest.config.ts",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "release": "dotenv release-it --",
    "dev": "pnpm run build",
    "build": "pnpm run build:clean && pnpm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "NODE_ENV=production rollup -c",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "lodash": "^4.17.21",
    "tinycolor2": "^1.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@jest/environment": "29.2.2",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.25",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-jest": "^29.2.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.2.2",
    "jsdom": "^20.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "rollup": "3.2.5",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
